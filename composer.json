{
    "name": "shopiumbot/common",
    "description": "Common application",
    "keywords": [],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/shopiumbot/common/issues?state=open",
        "source": "https://github.com/shopiumbot/common"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.30",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-jui": "dev-master",
        "yiisoft/yii2-smarty": "^2.0@dev",
        "yiisoft/yii2-bootstrap4": "^2.0.0",
        "yiisoft/yii2-debug": "~2",
        "panix/engine": "dev-master",
        "panix/mod-plugins": "dev-master",
        "panix/mod-seo": "dev-master",
        "panix/mod-admin": "dev-master",
        "panix/mod-discounts": "dev-master",
        "panix/mod-contacts": "dev-master",
        "panix/mod-telegram": "dev-master",
        "panix/mod-user": "dev-master",
        "panix/mod-shop": "dev-master",
        "panix/mod-images": "dev-master",
        "panix/mod-rbac": "dev-master"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "autoload": {
        "psr-4": {
            "core\\": ""
        }
    }
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
